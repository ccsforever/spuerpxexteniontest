{
	"name": "superpxextensiontest",
	"displayName": "superpxextensiontest",
	"description": "superpx extension test",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:superpxextensiontest.helloWorld",
		"onFileSystem:memfs",
    "onFileSystem:dbfs"
	],
	"main": "./out/extension.js",
  "publisher": "leeminho",
	"contributes": {
		"commands": [
			{
				"command": "superpxextensiontest.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "memfs.workspaceInit",
				"title": "Setup Workspace",
				"category": "MemFS"
			},
			{
				"command": "memfs.init",
				"title": "Create Files",
				"category": "MemFS"
			},
			{
				"command": "memfs.reset",
				"title": "Delete Files",
				"category": "MemFS"
			},
			{
				"command": "memfs.deleteFile",
				"title": "Delete \"file.txt\"",
				"category": "MemFS"
			},
			{
				"command": "memfs.addFile",
				"title": "Add \"file.txt\"",
				"category": "MemFS"
			},
			{
				"command": "memfs.initFromDB",
				"title": "Init DB fs",
				"category": "DBFS"
			},
			{
				"command": "memfs.getFromDB",
				"title": "Get file form DB ",
				"category": "DBFS"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "memfs.init",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.reset",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.deleteFile",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.addFile",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.workspaceInit",
					"when": "workbenchState != workspace"
				},
				{
					"command": "memfs.initFromDB",
					"when": "workbenchState != workspace"
				},
				{
					"command": "memfs.getFromDB",
					"when": "workbenchState == workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"ws": "^8.11.0"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@types/ws": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"@vscode/test-electron": "^2.1.5",
		"eslint": "^8.24.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.8.4"
	}
}
